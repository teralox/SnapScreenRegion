          </widget>
          <packing>
            <property name="X">187</property>
            <property name="Y">39</property>
          </packing>
        </child>
        <child>
          <widget class="Gtk.Image" id="ImageCanvasPic">
            <property name="MemberName" />
            <property name="WidthRequest">800</property>
            <property name="HeightRequest">500</property>
          </widget>
          <packing>
            <property name="X">192</property>
            <property name="Y">278</property>
          </packing>
        </child>
      </widget>
    </child>
  </widget>
</stetic-interface>